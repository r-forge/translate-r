msgstr ""

#. type: item
#: R-admin.texi:3846
#, no-wrap
msgid "--x-libraries=@var{DIR}"
msgstr "--x-libraries=@var{DIR}"

#. type: table
#: R-admin.texi:3848
msgid "X library files are in @var{DIR}"
msgstr ""

