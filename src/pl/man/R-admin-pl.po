#. type: Plain text
#: R-admin.texi:3832
msgid "@command{configure} has many options: running"
msgstr ""

#. type: example
#: R-admin.texi:3835
#, no-wrap
msgid "./configure --help\n"
msgstr "./configure --help\n"

#. type: Plain text
#: R-admin.texi:3840
msgid "will give a list.  Probably the most important ones not covered elsewhere are (defaults in brackets)"
msgstr ""

#. type: item
#: R-admin.texi:3842
#, no-wrap
msgid "--with-x"
msgstr "--with-x"

#. type: table
#: R-admin.texi:3844
msgid "use the X Window System [yes]"
msgstr ""

#. type: item
#: R-admin.texi:3844
#, no-wrap
